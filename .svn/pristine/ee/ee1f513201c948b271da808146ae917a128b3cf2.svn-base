package com.dfws.shhreader.database.property;
/**
 * <h2>下载相关属性配置</h2>
 * <pre>包括离线下载断点续传</pre>
 * @author Eilin.Yang
 * @since 2013-10-22
 * @version v1.0
 */
public class DownLoadProperty {
	
	/**1612606282
	 * int型文件ID
	 */
	public int id;
	/**
	 * String型文件ID
	 */
	public String idstr;
	/**
	 * 文件名
	 */
	public String name;
	/**
	 * 文件总大小
	 */
	public int size;
	/**
	 * 已经完成的进度
	 */
	public int localsize;
	/**
	 * 文件网络地址
	 */
	public String url;
	/**
	 * 本地路径
	 */
	public String path;
	/**
	 * 文件后缀名
	 */
	public String suffix;
	/**
	 * 是否已经下载完成
	 */
	public boolean isfinish;
	/**
	 * 开始下载时间
	 */
	public String startDateTime;
	/**
	 * 下载完成时间
	 */
	public String finishDateTime;
	/**
	 * 需要总的时间
	 */
	public long takeTime;
}
