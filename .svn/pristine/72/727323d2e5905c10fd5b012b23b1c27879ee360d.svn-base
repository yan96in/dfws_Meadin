package com.dfws.shhreader.entity;

import java.util.List;

/**
 * <h2>新闻对象</h2>
 * 每一条新闻是一个新闻对象
 * @author Eilin.Yang
 * @since 2013-9-18
 * @version v1.0
 */
public class News {

	/**
	 * 新闻id
	 */
	private int id;
	/**
	 * 新闻String类型id
	 */
	private String idStr;
	/**
	 * 新闻标题
	 */
	private String title;
	/**
	 * 新闻简介
	 */
	private String digest;
	/**
	 * 新闻详细
	 */
	private String content;
	/**
	 * 新闻来源
	 */
	private String source;
	/**
	 * 新闻作者
	 */
	private String author;
	/**
	 * 新闻网络地址
	 */
	private String url;
	/**
	 * 新闻本地地址
	 */
	private String path;
	/**
	 * 是否已读
	 */
	private boolean isReaded;
	/**
	 * 是否已经收藏.true：已收藏，false：没有收藏
	 */
	private boolean isCollected;
	/**
	 * 新闻类型
	 */
	private int type;
	/**
	 * 新闻栏目
	 */
	private int column;
	/**
	 * 发布时间
	 */
	private String potime;
	/**
	 * 收藏时间
	 */
	private String fvtime;
	/**
	 * 取消收藏时间
	 */
	private String unfvtime;
	/**
	 * 缩略图地址
	 */
	private String thumb;	
	/**
	 * 评论总数
	 */
	private int comment_count;
	/**
	 * 媒体
	 */
	private List<MediaInfo> medias;
	
	public News() {
		
	}
	/**
	 * 
	 *<pre>构造新闻</pre>
	 * @param id 新闻id
	 * @param idStr 新闻id
	 * @param title 标题
	 * @param digest 导语
	 * @param content 详细内容
	 * @param source 来源
	 * @param author 作者
	 * @param url 网络地址 
	 * @param path 本地地址
	 * @param isReaded 是否已经阅读过
	 * @param isCollected 是否已经收藏
	 * @param type 新闻媒体类型，1：普通新闻、2：图片新闻。
	 * @param column 所属栏目。1：热点、2：图片、3：报告、 4：人物、5：新技术，默认为1。
	 * @param potime 发布时间
	 * @param fvtime 收藏时间
	 * @param unfvtime 取消收藏时间
	 * @param thumb 缩略图地址
	 * @param comment_count 评论总数
	 * @param medias 媒体对象，包括图片
	 */
	public News(int id, String idStr, String title, String digest,
			String content, String source, String author, String url,
			String path, boolean isReaded, boolean isCollected, int type,
			int column, String potime, String fvtime, String unfvtime,
			String thumb, int comment_count, List<MediaInfo> medias) {
		super();
		this.id = id;
		this.idStr = idStr;
		this.title = title;
		this.digest = digest;
		this.content = content;
		this.source = source;
		this.author = author;
		this.url = url;
		this.path = path;
		this.isReaded = isReaded;
		this.isCollected = isCollected;
		this.type = type;
		this.column = column;
		this.potime = potime;
		this.fvtime = fvtime;
		this.unfvtime = unfvtime;
		this.thumb = thumb;
		this.comment_count = comment_count;
		this.medias = medias;
	}
	/**
	 * 
	 *<pre>id</pre>
	 * @return
	 */
	public int getId() {
		return id;
	}
	/**
	 * 
	 *<pre>id</pre>
	 * @param id
	 */
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * 
	 *<pre>id</pre>
	 * @return
	 */
	public String getIdStr() {
		return idStr;
	}
	/**
	 * 
	 *<pre>id</pre>
	 * @param idStr
	 */
	public void setIdStr(String idStr) {
		this.idStr = idStr;
	}
	/**
	 * 
	 *<pre>标题</pre>
	 * @return
	 */
	public String getTitle() {
		return title;
	}
	/**
	 * 
	 *<pre>标题</pre>
	 * @param title
	 */
	public void setTitle(String title) {
		this.title = title;
	}
	/**
	 * 	
	 *<pre>导语</pre>
	 * @return
	 */
	public String getDigest() {
		return digest;
	}
	/**
	 * 
	 *<pre>导语</pre>
	 * @param digest
	 */
	public void setDigest(String digest) {
		this.digest = digest;
	}
	/**
	 * 
	 *<pre>内容</pre>
	 * @return
	 */
	public String getContent() {
		return content;
	}
	/**
	 * 
	 *<pre>内容</pre>
	 * @param content
	 */
	public void setContent(String content) {
		this.content = content;
	}
	/**
	 * 
	 *<pre>来源</pre>
	 * @return
	 */
	public String getSource() {
		return source;
	}
	/**
	 * 
	 *<pre>来源</pre>
	 * @param source
	 */
	public void setSource(String source) {
		this.source = source;
	}
	/**
	 * 
	 *<pre>作者</pre>
	 * @return
	 */
	public String getAuthor() {
		return author;
	}
	/**
	 * 
	 *<pre>作者</pre>
	 * @param author
	 */
	public void setAuthor(String author) {
		this.author = author;
	}
	/**
	 * 
	 *<pre>网络地址</pre>
	 * @return
	 */
	public String getUrl() {
		return url;
	}
	/**
	 * 
	 *<pre>网络地址</pre>
	 * @param url
	 */
	public void setUrl(String url) {
		this.url = url;
	}
	/**
	 * 
	 *<pre>本地地址</pre>
	 * @return
	 */
	public String getPath() {
		return path;
	} 
	/**
	 * 
	 *<pre>本地地址</pre>
	 * @param path
	 */
	public void setPath(String path) {
		this.path = path;
	}
	/**
	 * 
	 *<pre>是否已经阅读</pre>
	 * @return
	 */
	public boolean isReaded() {
		return isReaded;
	}
	/**
	 * 
	 *<pre>是否已经阅读</pre>
	 * @param isReaded
	 */
	public void setReaded(boolean isReaded) {
		this.isReaded = isReaded;
	}
	/**
	 * 
	 *<pre>是否已经收藏</pre>
	 * @return
	 */
	public boolean isCollected() {
		return isCollected;
	}
	/**
	 * 
	 *<pre>是否已经收藏</pre>
	 * @param isCollected
	 */
	public void setCollected(boolean isCollected) {
		this.isCollected = isCollected;
	}
	/**
	 * 
	 *<pre>新闻媒体类型
	 *1：普通新闻、
	 *2：图片新闻。</pre>
	 * @return
	 */
	public int getType() {
		return type;
	}
	/**
	 *<pre>新闻媒体类型
	 *1：普通新闻、
	 *2：图片新闻。</pre>
	 * @param type
	 */
	public void setType(int type) {
		this.type = type;
	}
	/**
	 * 
	 *<pre>所属栏目
	 *新闻栏目类型，
	 *1：热点、
	 *2：图片、
	 *3：报告、
	 * 4：人物、
	 * 5：新技术，
	 * 默认为1。</pre>
	 * @return
	 */
	public int getColumn() {
		return column;
	}
	/**
	 *<pre>所属栏目
	 *新闻栏目类型，
	 *1：热点、
	 *2：图片、
	 *3：报告、
	 * 4：人物、
	 * 5：新技术，
	 * 默认为1。</pre>
	 * @param column
	 */
	public void setColumn(int column) {
		this.column = column;
	}
	/**
	 * 
	 *<pre>发布时间</pre>
	 * @return
	 */
	public String getPotime() {
		return potime;
	}
	/**
	 * 
	 *<pre>发布时间</pre>
	 * @param potime
	 */
	public void setPotime(String potime) {
		this.potime = potime;
	}
	/**
	 * 
	 *<pre>收藏时间</pre>
	 * @return
	 */
	public String getFvtime() {
		return fvtime;
	}
	/**
	 * 
	 *<pre>收藏时间</pre>
	 * @param fvtime
	 */
	public void setFvtime(String fvtime) {
		this.fvtime = fvtime;
	}
	/**
	 * 
	 *<pre>取消收藏时间</pre>
	 * @return
	 */
	public String getUnfvtime() {
		return unfvtime;
	}
	/**
	 * 
	 *<pre>取消收藏时间</pre>
	 * @param unfvtime
	 */
	public void setUnfvtime(String unfvtime) {
		this.unfvtime = unfvtime;
	}
	/**
	 * 
	 *<pre>缩略图地址</pre>
	 * @return
	 */
	public String getThumb() {
		return thumb;
	}
	/**
	 * 
	 *<pre>缩略图地址</pre>
	 * @param thumb
	 */
	public void setThumb(String thumb) {
		this.thumb = thumb;
	}
	/**
	 * 
	 *<pre>评论总数</pre>
	 * @return
	 */
	public int getComment_count() {
		return comment_count;
	}
	/**
	 * 
	 *<pre>评论总数</pre>
	 * @param comment_count
	 */
	public void setComment_count(int comment_count) {
		this.comment_count = comment_count;
	}
	/**
	 * 
	 *<pre>媒体信息</pre>
	 * @return
	 */
	public List<MediaInfo> getMedias() {
		return medias;
	}
	/**
	 * 
	 *<pre>媒体信息</pre>
	 * @param medias
	 */
	public void setMedias(List<MediaInfo> medias) {
		this.medias = medias;
	}
	@Override
	public String toString() {
		return "News [id=" + id + ", idStr=" + idStr + ", title=" + title
				+ ", digest=" + digest + ", content=" + content + ", source="
				+ source + ", author=" + author + ", url=" + url + ", path="
				+ path + ", isReaded=" + isReaded + ", isCollected="
				+ isCollected + ", type=" + type + ", column=" + column
				+ ", potime=" + potime + ", fvtime=" + fvtime + ", unfvtime="
				+ unfvtime + ", thumb=" + thumb + ", comment_count="
				+ comment_count + ", medias=" + medias + "]";
	}	
}
