package com.dfws.shhreader.entity;
/**
 * 
 * <h2>媒体对象 <h2>
 * <pre> </pre>
 * @author 东方网升Eilin.Yang
 * @since 2013-10-24
 * @version v1.0
 * @modify ""
 */
public class MediaInfo {
	/**
	 * 新闻id
	 */
	private int news_id;
	/**
	 * 图片id
	 */
	private String id;
	/**
	 * 标签
	 */
	private String ref;
	/**
	 * 宽
	 */
	private int width;
	/**
	 * 高
	 */
	private int height;
	
	/**
	 * 图片网络地址
	 */
	private String src;
	/**
	 * 图片本地地址
	 */
	private String path;
	/**
	 * 图片说明
	 */
	private String alt;
	public MediaInfo() {
		
	}
	/**
	 * 
	 *<pre>封装媒体信息</pre>
	 * @param news_id 新闻id
	 * @param ref 标签位置
	 * @param width 图片宽
	 * @param height 图片高
	 * @param src 网络地址
	 * @param path 本地地址
	 * @param alt 说明
	 */
	public MediaInfo(int news_id, String ref, int width, int height,
			String src, String path, String alt) {
		super();
		this.news_id = news_id;
		this.ref = ref;
		this.width = width;
		this.height = height;
		this.src = src;
		this.path = path;
		this.alt = alt;
	}
	/**
	 * 
	 *<pre>新闻id</pre>
	 * @return
	 */
	public int getNews_id() {
		return news_id;
	}
	/**
	 * 
	 *<pre>新闻id</pre>
	 * @param news_id
	 */
	public void setNews_id(int news_id) {
		this.news_id = news_id;
	}
	/**
	 * <pre>图片id</pre>
	 * @return
	 */
	public String getId() {
		return id;
	}
	/**
	 * <pre>图片id</pre>
	 * @param id
	 */
	public void setId(String id) {
		this.id = id;
	}
	/**
	 * 
	 *<pre>位置标签</pre>
	 * @return
	 */
	public String getRef() {
		return ref;
	}
	/**
	 * 
	 *<pre>位置标签</pre>
	 * @param ref
	 */
	public void setRef(String ref) {
		this.ref = ref;
	}
	/**
	 * 
	 *<pre>宽</pre>
	 * @return
	 */
	public int getWidth() {
		return width;
	}
	/**
	 * 
	 *<pre>宽</pre>
	 * @param width
	 */
	public void setWidth(int width) {
		this.width = width;
	}
	/**
	 * 
	 *<pre>高</pre>
	 * @return
	 */
	public int getHeight() {
		return height;
	}
	/**
	 * 
	 *<pre>高</pre>
	 * @param height
	 */
	public void setHeight(int height) {
		this.height = height;
	}
	/**
	 * 
	 *<pre>网络地址</pre>
	 * @return
	 */
	public String getSrc() {
		return src;
	}
	/**
	 * 
	 *<pre>网络地址</pre>
	 * @param src
	 */
	public void setSrc(String src) {
		this.src = src;
	}
	/**
	 * 
	 *<pre>本地地址</pre>
	 * @return
	 */
	public String getPath() {
		return path;
	}
	/**
	 * 
	 *<pre>本地地址</pre>
	 * @param path
	 */
	public void setPath(String path) {
		this.path = path;
	}
	/**
	 * 
	 *<pre>图片说明</pre>
	 * @return
	 */
	public String getAlt() {
		return alt;
	}
	/**
	 * 
	 *<pre>图片说明</pre>
	 * @param alt
	 */
	public void setAlt(String alt) {
		this.alt = alt;
	}
	@Override
	public String toString() {
		return "MediaInfo [news_id=" + news_id + ", id=" + id + ", ref=" + ref
				+ ", width=" + width + ", height=" + height + ", src=" + src
				+ ", path=" + path + ", alt=" + alt + "]";
	}
}
